{
    "dataRoles": [
        {
            "displayName": "Locations",
            "name": "category",
            "kind": "Grouping"
        },
        {
            "displayName": "Values",
            "name": "measure",
            "kind": "Measure"
        },
        {
            "displayName": "Tooltip",
            "name": "tooltipmeasure",
            "kind": "Measure"
        },
        {
            "displayName": "Legend",
            "name": "Legend",
            "kind": "GroupingOrMeasure"
        }
    ],
    "dataViewMappings": [
        {
            "conditions": [
                {
                    "category": {
                        "max": 3
                    },
                    "measure": {
                        "min": 0,
                        "max": 1
                    },
                    "Legend": {
                        "min": 0,
                        "max": 1
                    }
                }
            ],
            "categorical": {
                "categories": {
                    "dataReductionAlgorithm": {
                        "top": {
                            "count": 80000
                        }
                    },
                    "for": {
                        "in": "category"
                    }
                },
                "values": {
                    "select": [
                        {
                            "bind": {
                                "to": "measure"
                            }
                        },
                        {
                            "bind": {
                                "to": "tooltipmeasure"
                            }
                        },
                        {
                            "bind": {
                                "to": "Legend"
                            }
                        }
                    ]
                }
            }
        }
    ],
    "supportsHighlight": true,
    "drilldown": {
        "roles": [
            "category"
        ]
    },
